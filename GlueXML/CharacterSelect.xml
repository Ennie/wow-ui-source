					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</HighlightTexture>
				<Scripts>
					<OnClick>
						local index = self:GetParent().index;
						MoveCharacter(index, index - 1);
					</OnClick>
				</Scripts>
